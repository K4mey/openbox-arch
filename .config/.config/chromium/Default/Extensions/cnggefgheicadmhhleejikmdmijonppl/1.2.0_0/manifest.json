{
   "app": {
      "background": {
         "scripts": [ "./dist/background.bundle.js" ]
      }
   },
   "description": "Queue videos from all over the web and your local harddrive to play in a customizable player or on your ChromeCast.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "29",
   "name": "Queue Player",
   "permissions": [ "\u003Call_urls>", "storage", "background", "app.window.fullscreen", {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "idle", "alwaysOnTopWindows", "system.network", "retainEntries", "webview", "system.network", "browser" ],
   "sockets": {
      "tcp": {
         "connect": "*:*"
      },
      "tcpServer": {
         "listen": "*:*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0",
   "webview": {
      "partitions": [ {
         "accessible_resources": [ "/layout/youtube-player.html", "/dist/app.css", "/dist/youtubeplayer.bundle.js", "/Libs/Youtube/youtube-iframe-api.js", "/Libs/Youtube/widgetapi.js" ],
         "name": "static"
      } ]
   }
}
