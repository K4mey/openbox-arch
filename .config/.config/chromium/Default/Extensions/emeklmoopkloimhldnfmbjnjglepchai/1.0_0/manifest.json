{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content.css" ],
      "js": [ "jquery.min.js", "content.js" ],
      "matches": [ "*://4chan.org/*", "*://*.4chan.org/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com  https://ssl.google-analytics.com https://connect.facebook.net; object-src 'self'",
   "description": "The best way to browse 4chan.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "4chan Plus",
   "options_page": "options.html",
   "permissions": [ "*://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [ "icon48.png", "img/loader.gif", "img/404.png", "img/report.png", "img/x.png", "img/quickrep.png", "img/min.png", "img/expand.png", "img/refresh.png", "img/bookmarks.png", "img/star.png", "img/play.png", "img/awwyeah.png", "img/close.png" ]
}
